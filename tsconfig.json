{
    "compilerOptions": {
        "target": "es2020",
        "module": "NodeNext",
        "lib": ["esnext"],
        "moduleResolution": "nodenext",
        "esModuleInterop": true,
        "strict": true,
        "strictNullChecks": true,
        "resolveJsonModule": true,
        "skipLibCheck": true,
        "skipDefaultLibCheck": true
    }
}
